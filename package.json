{
  "name": "rx-player",
  "author": "Canal+",
  "version": "2.0.12",
  "description": "Canal+ HTML5 Video Player",
  "main": "./dist/rx-player.min.js",
  "keywords": [
    "video",
    "streaming",
    "html5",
    "mediasource",
    "dash",
    "reactive",
    "rx"
  ],
  "browser": "./dist/rx-player.js",
  "homepage": "https://github.com/canalplus/rx-player",
  "bugs": "https://github.com/canalplus/rx-player/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack --progress --config webpack.config.js src/index.js dist/rx-player.js",
    "build:min": "RXP_MINIFY=true webpack --progress --config webpack.config.js src/index.js dist/rx-player.min.js",
    "build:watch": "webpack --progress --config webpack.config.js -w src/index.js dist/rx-player.js",
    "lint": "eslint ./src/**/*.js",
    "karma": "karma start test/karma.conf.js",
    "coverage": "RXP_COVERAGE=true karma start test/karma.conf.js --browsers Chrome,Firefox",
    "karma-chrome": "karma start test/karma.conf.js --browsers Chrome",
    "karma-firefox": "karma start test/karma.conf.js --browsers Firefox",
    "demo": "webpack --progress --config webpack-demo.config.js",
    "demo:watch": "webpack --progress --config webpack-demo.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/canalplus/rx-player.git"
  },
  "dependencies": {
    "rxjs": "5.0.0-rc.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "chai-spies": "^0.5.1",
    "chart.js": "^2.5.0",
    "coverjs-loader": "^0.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.8.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsx-loader": "^0.13.1",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "^2.0.3",
    "lodash": "4.11.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rimraf": "^2.5.2",
    "script-loader": "^0.6.1",
    "selenium-webdriver": "^2.53.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.12.12",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^1.14.1",
    "xunit-file": "0.0.9"
  }
}
